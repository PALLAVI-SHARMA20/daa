#include<bits/stdc++.h>
using namespace std;

int main() 
{ 
    int start[] =  {1, 3, 0, 5, 3, 5, 6, 8, 8, 2, 12}; 
    int finish[] =  {4, 5, 6, 7, 9, 9, 10, 11, 12, 14, 16}; 
    int i=0;
    int n=11;
    cout<<i+1<<" ";
    for(int j=1;j<n;j++)
    {
        if(finish[i] <= start[j])
     {
        cout<<j+1<<" ";
        i=j;
     }
    }
    
    return 0; 
} 
//o/p: 1 4 8 11
